{
  "name": "SO Dark Chat +",
  "description": "Provides a new dark theme for stackoverflow chat.",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://chat.stackoverflow.com/*",
        "*://chat.stackexchange.com/*",
        "*://chat.meta.stackexchange.com/*"
      ],
      "js": ["dist/main.js"],
      "css": ["dist/main.css"]
    }
  ],
  "icons": {
    "128": "img/icon_128.png"
  },
  "permissions": ["storage"]
}
