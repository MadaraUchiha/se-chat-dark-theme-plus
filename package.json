{  "name": "se-chat-dark-theme-plus",  "version": "1.1.0",  "description": "'dark chat theme for stackoverflow'",  "scripts": {    "build": "rimraf dist && webpack --progress",    "release": "rimraf release dist && mkdir release && zip -r release/se-dark-chat-theme-plus-source.zip ./* -x=*node_modules* && yarn build --mode=production && zip -r release/se-chat-dark-theme-plus.zip manifest.json dist/ img/",    "bump": "ts-node build/bump-version"  },  "devDependencies": {    "@types/chrome": "^0.0.59",    "@types/codemirror": "^0.0.71",    "@types/mini-css-extract-plugin": "^0.2.1",    "@types/webpack": "^4.4.24",    "css-loader": "^3.0.0",    "cssimportant-loader": "^0.4.0",    "husky": "^3.0.1",    "lint-staged": "9.2.0",    "mini-css-extract-plugin": "^0.7.0",    "node-sass": "^4.12.0",    "prettier": "1.18.2",    "rimraf": "^2.6.3",    "sass-loader": "^7.1.0",    "ts-loader": "^5.3.3",    "ts-node": "^8.0.2",    "typescript": "^3.3.1",    "webpack": "^4.29.0",    "webpack-cli": "^3.2.3"  },  "author": "rlemon",  "license": "ISC",  "dependencies": {    "codemirror": "^5.43.0"  },  "husky": {    "hooks": {      "pre-commit": "lint-staged"    }  },  "lint-staged": {    "*.{js,css,json,md}": [      "prettier --write",      "git add"    ]  }}